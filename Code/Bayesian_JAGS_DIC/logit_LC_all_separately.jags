model{
  for (i in 1:p){
    for (j in 1:A){
      for (k in 1:T){
        dxt[i,j,k]~dbin(q[i,j,k],ext[i,j,k])
        logit(q[i,j,k])<-alpha[i,j]+beta[i,j] * kappa[i,k]
      }
    }
  }

  for (i in 1:p){
  for (j in 2:T){
  kappa[i,j]<-kappa_rest_mat[i,j-1]
  }

  kappa[i,1]<--sum(kappa_rest_mat[i,])

  }

  for (i in 1:p){
  for (j in 1:(T-1)){
  kappa_rest_mat[i,j]~dnorm(0,0.001)
  }
  }

  for (i in 1:p){
  for (j in 2:A){
  beta[i,j]<-beta_rest_mat[i,j-1]
  }

  beta[i,1]<-1-sum(beta_rest_mat[i,])

  }

  for (i in 1:p){
  for (j in 1:(A-1)){
  beta_rest_mat[i,j]~dnorm(1/A,10000)
  }
  }
  
  for (i in 1:p){
  for (j in 1:A){
  alpha[i,j]~dnorm(0,0.01)
  }
  }

}
